{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node  build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "prod:start": "npm run prod:build-client && npm run prod:build-server && npm run prod:server",
    "prod:server": "node build/bundle.js",
    "prod:build-server": "webpack --config webpack.prod.server.js",
    "prod:build-client": "webpack --config webpack.prod.client.js",
    "lint:less": "stylelint 'src/**/*.less' --syntax less",
    "lint:less:fix": "stylelint 'src/**/*.less' --syntax less --fix",
    "lint:js": "eslint src/ --ext .js,.jsx",
    "lint:js:fix": "eslint src/ --ext .js,.jsx --fix"
  },
  "author": "V.V.Arkavenko",
  "license": "MIT",
  "description": "Good client architecture",
  "browserslist": [
    "> 1%",
    "ie 10"
  ],
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "react-loadable/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:js:fix",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:less:fix",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "cors": "^2.8.5",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-loader": "^2.2.1",
    "express": "^4.17.3",
    "express-http-proxy": "^1.5.1",
    "html-loader": "^0.5.5",
    "i18next": "^17.0.6",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "node": "^12.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.5",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
